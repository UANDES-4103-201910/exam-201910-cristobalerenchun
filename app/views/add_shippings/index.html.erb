<p id="notice"><%= notice %></p>

<h1>Add Shippings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Order</th>
      <th>Phone</th>
      <th>Addline1</th>
      <th>Addline2</th>
      <th>City</th>
      <th>Country</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_shippings.each do |add_shipping| %>
      <tr>
        <td><%= add_shipping.user %></td>
        <td><%= add_shipping.order %></td>
        <td><%= add_shipping.phone %></td>
        <td><%= add_shipping.addline1 %></td>
        <td><%= add_shipping.addline2 %></td>
        <td><%= add_shipping.city %></td>
        <td><%= add_shipping.country %></td>
        <td><%= add_shipping.zipcode %></td>
        <td><%= link_to 'Show', add_shipping %></td>
        <td><%= link_to 'Edit', edit_add_shipping_path(add_shipping) %></td>
        <td><%= link_to 'Destroy', add_shipping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add Shipping', new_add_shipping_path %>
