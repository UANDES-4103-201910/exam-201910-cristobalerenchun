<p id="notice"><%= notice %></p>

<h1>Add Billings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Order</th>
      <th>Phone</th>
      <th>Addline1</th>
      <th>Addline2</th>
      <th>City</th>
      <th>Country</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_billings.each do |add_billing| %>
      <tr>
        <td><%= add_billing.user %></td>
        <td><%= add_billing.order %></td>
        <td><%= add_billing.phone %></td>
        <td><%= add_billing.addline1 %></td>
        <td><%= add_billing.addline2 %></td>
        <td><%= add_billing.city %></td>
        <td><%= add_billing.country %></td>
        <td><%= add_billing.zipcode %></td>
        <td><%= link_to 'Show', add_billing %></td>
        <td><%= link_to 'Edit', edit_add_billing_path(add_billing) %></td>
        <td><%= link_to 'Destroy', add_billing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add Billing', new_add_billing_path %>
